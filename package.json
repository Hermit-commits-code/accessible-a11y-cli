{
  "name": "accessible-a11y-cli",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.4.0",
  "description": "A robust, developer-friendly CLI for accessibility testing and autofix with axe-core. Features advanced autofix logic, clear output, safe file handling, and extensive tests.",
  "main": "src/index.js",
  "bin": {
    "a11y-check": "./bin/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src/ bin/ test/",
    "lint:fix": "eslint src/ bin/ test/ --fix",
    "format": "prettier --write src/ bin/ test/",
    "start": "node bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hermit-commits-code/accessible-a11y-cli.git"
  },
  "keywords": [
    "accessibility",
    "a11y",
    "axe",
    "cli",
    "testing",
    "html",
    "jsx",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hermit-commits-code/accessible-a11y-cli/issues"
  },
  "homepage": "https://github.com/Hermit-commits-code/accessible-a11y-cli#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "canvas": "^3.2.0",
    "chalk": "^4.1.2",
    "commander": "^14.0.1",
    "figlet": "^1.9.3",
    "jsdom": "^27.0.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "axe-core": "^4.10.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2"
  }
}
